1. insert username, email, bio
2. insert post with hashtag
limit 1000
3. show single hashtag with post
4. shos trending hashtag
limit 5 in 24 hour
limit 1 post
5. insert comment with hashtag
hashtag also trending
6. insert media







insert pos
cek list hast
foreach hash
 id hash = get id hash
 if id hash empty
  id hash = insert hash ke db
 end
 insert hastag_post(id_post, id_hash)
end


select tb_hashtag_post.id_hashtag as id_hashtag, tb_post.waktu as waktu from tb_hashtag_post left join tb_post on tb_hashtag_post.id_post = tb_post.id where waktu > (NOW() - INTERVAL 24 HOUR) group by tb_hashtag_post.id_hashtag order by count(tb_hashtag_post.id_hashtag) desc;
